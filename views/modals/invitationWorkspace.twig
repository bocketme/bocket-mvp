<div id="invite-workspace" class="modal">
  <div class="row container">
    <h4 style="padding-top: 30px">Invitation
      <a class="modal-close" style="color: black;">
        <i class="material-icons right modal-close">highlight_off</i>
      </a>
    </h4>
    <div class="row">
      <div class="col s12">
        <ul id="workspace-invitation-tabs" class="tabs">
          <li class="tab col s6">
            <a href="#member"> Organization Member </a>
          </li>
          <li class="tab col s6">
            <a href="#mail"> By Mail </a>
          </li>
        </ul>
        <div id="member">
          <div id="content-workspace-invitation">
          
          </div>
        </div>
        <div id="mail">
            <form id="invite-people-form">
            <input type="hidden" id="hidden-Workspace" value>
              <div id="list-member-invitation" class="col s12" style="padding: 30px 0">
              </div>
              <div class="col s12" style="padding: 30px 0">
                <a onclick="addMember()" style="font-size: 14px; color: #4A90E2; cursor: pointer;">
                  <i class="material-icons" style="vertical-align: middle">add_circle_outline</i>
                  Add an user
                </a>
              </div>
              <div class="col s12" style="padding: 30px 0">
                <a class="modal-close btn-cancel btn col s3 push-s2">Cancel</a>
                <button id="invite-people-submit" class="modal-close btn-confirm btn col s3 push-s4">Invite</button>
              </div>
            </form>        
        </div>
      </div>
    </div>
  </div>
</div>
